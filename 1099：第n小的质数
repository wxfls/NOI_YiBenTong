/***
【题目描述】
输入一个正整数n，求第n小的质数。

【输入】
一个不超过10000的正整数n。

【输出】
第n小的质数。

【输入样例】
10
【输出样例】
29
***/

#include<iostream>

using namespace std;
int main()
{
   int n;
   cin>>n;
   bool flag;
   int sum=0;

   for(int i=2;;i++)
   {
        flag=false;
        for(int j=2;2*j<=i;j++)//
        {
            if(i%j==0)
            {
                flag=true;
                break;
            }
        }
        if(flag==false)
        {
            sum++;
            if(sum==n)
            {
                cout<<i;
                return 0;
            }
        }
   }
    return 0;
}
